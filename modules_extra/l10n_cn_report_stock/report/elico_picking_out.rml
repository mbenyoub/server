    <section>
      <para style="terp_default_2">[[ repeatIn([line for line in picking.move_lines if (line.product_qty &gt;0 and (line.state == 'draft' or line.state=='waiting'))],'move_lines') ]]</para>
      <para style="terp_default_2">[[ (picking.move_lines!=[] and removeParentNode('para')) or removeParentNode('section')]]</para>
      <blockTable colWidths="16.0,182.0,83.0,55.0,54.0,56.0,54.0" style="Table1">
