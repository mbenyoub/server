<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="sequence_sale_commission_commission_type" model="ir.sequence.type">
            <field name="name">Comision calculada</field>
            <field name="code">sale.commission.commission</field>
        </record>
        
        <record id="sequence_sale_commission_commission" model="ir.sequence">
            <field name="name">Comision acumulada</field>
            <field name="code">sale.commission.commission</field>
            <field name="prefix">COMM/%(month)s/%(year)s/</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</openerp>