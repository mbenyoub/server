<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        
        <!--
        Account Journal Sequences
        -->

        <record id="account.sequence_sale_journal" model="ir.sequence">
            <field name="name">Account Default Sales Journal</field>
            <field eval="3" name="padding"/>
            <field name="prefix">SAJ/%(year)s/%(month)s/</field>
        </record>
        <record id="account.sequence_refund_sales_journal" model="ir.sequence">
            <field name="name">Account Default Sales Credit Note Journal</field>
            <field eval="3" name="padding"/>
            <field name="prefix">SCNJ/%(year)s/%(month)s/</field>
        </record>
        <record id="account.sequence_purchase_journal" model="ir.sequence">
            <field name="name">Account Default Expenses Journal</field>
            <field eval="3" name="padding"/>
            <field name="prefix">EXJ/%(year)s/%(month)s/</field>
        </record>
        <record id="account.sequence_refund_purchase_journal" model="ir.sequence">
            <field name="name">Account Default Expenses Credit Notes Journal</field>
            <field eval="3" name="padding"/>
            <field name="prefix">ECNJ/%(year)s/%(month)s/</field>
        </record>
        <record id="account.sequence_bank_journal" model="ir.sequence">
            <field name="name">Account Default Bank Journal</field>
            <field eval="3" name="padding"/>
            <field name="prefix">BNK/%(year)s/%(month)s/</field>
        </record>
        <record id="account.sequence_check_journal" model="ir.sequence">
            <field name="name">Account Default Checks Journal</field>
            <field eval="3" name="padding"/>
            <field name="prefix">CHK/%(year)s/%(month)s/</field>
        </record>
        <record id="account.sequence_cash_journal" model="ir.sequence">
            <field name="name">Account Default Cash Journal</field>
            <field eval="3" name="padding"/>
            <field name="prefix">CSH/%(year)s/%(month)s/</field>
        </record>
        <record id="account.sequence_opening_journal" model="ir.sequence">
            <field name="name">Account Default Opening Entries Journal</field>
            <field eval="3" name="padding"/>
            <field name="prefix">OPEJ/%(year)s/%(month)s/</field>
        </record>
        <record id="account.sequence_miscellaneous_journal" model="ir.sequence">
            <field name="name">Account Default Miscellaneous Journal</field>
            <field eval="3" name="padding"/>
            <field name="prefix">MISJ/%(year)s/%(month)s/</field>
        </record>
        
    </data>
</openerp>
