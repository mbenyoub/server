<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        
        <!--
        Requisition Credit Sequences
        -->
        
        <record id="af_expense_sequence_statement_type" model="ir.sequence.type">
            <field name="name">Requisition Credit Default Folio</field>
            <field name="code">requisition.credit</field>
        </record>
        <record id="sequence_requisition_credit" model="ir.sequence">
            <field name="name">Requisition Credit Default Folio</field>
            <field name="code">requisition.credit</field>
            <field name="prefix">CREDIT/%(year)s/%(month)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="3" name="padding"/>
        </record>
        
    </data>
</openerp>
